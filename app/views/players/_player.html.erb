<div>
  <tr>
    <td><%= link_to player.fullname, player_path(player) %></td>
    <td><%= player.position %></td>
    <td><%= link_to player.pro_team.full_name, pro_team_path(player.pro_team) %></td>
    <td><% if signed_in? %>
          <div id="pick_player_button">
            <%= render "pick_player_button", player: player %>
          </div>
        <% end %>
  </tr>
</div>

